{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "2.2.6",
    "coveralls": "3.0.3",
    "highlight.js": "9.15.6",
    "http-proxy-middleware": "0.19.1",
    "is-git-url": "1.0.0",
    "lodash": "4.17.11",
    "lodash-es": "4.17.11",
    "moment": "2.24.0",
    "moment-duration-format": "2.2.2",
    "moment-precise-range-plugin": "1.3.0",
    "node-forge": "0.8.2",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.5",
    "patternfly": "3.59.1",
    "patternfly-react": "2.33.0",
    "qrcode.react": "0.9.3",
    "react": "16.8.6",
    "react-copy-to-clipboard": "5.0.1",
    "react-debounce-input": "3.2.0",
    "react-dom": "16.8.6",
    "react-jsonschema-form": "1.3.0",
    "react-markdown": "4.0.6",
    "react-moment": "0.8.4",
    "react-redux": "5.1.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coverage": "react-scripts test --env=jsdom --coverage --maxWorkers 4 --transformIgnorePatterns \"node_modules/(?!lodash-es)/\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules/patternfly/dist/sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome-sass/assets/stylesheets ./src/ -o ./src/ --watch --recursive",
    "lint": "eslint 'src/**/*.js'"
  },
  "homepage": "/",
  "devDependencies": {
    "cors-anywhere": "0.4.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.16.0",
    "eslint-plugin-patternfly-react": "0.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "fetch-mock": "7.3.1",
    "jest": "24.8.0",
    "prettier": "1.16.4",
    "redux-mock-store": "1.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
